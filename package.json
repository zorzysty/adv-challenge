    "react-scripts": "^4.0.3",
    "typescript": "^4.3.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "format": "prettier --write --ignore-path .gitignore src/**/*.{js,ts,jsx,tsx,json,md}",
    "lint": "eslint --ignore-path .gitignore src/**/*.{js,ts,jsx,tsx}",
    "type-check": "tsc",
    "test": "react-scripts test",
    "validate": "yarn run format && yarn run lint --max-warnings 0 && yarn run type-check && yarn run test --watchAll=false",
    "prepare": "husky install"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  }
